let states = load('misal.builtin.states')
let distances = load('misal.builtin.dist')
let transforms = load('misal.builtin.transforms')
let actions = load('misal.builtin.actions')

let path_to_metadata = 'path_to_metadata'
let metadata = states.Metadata(path_to_metadata)
let preprocessed = states.Preprocessed({'str': 'dict'})
let index = states.Index(('tuple', 'of', 'str'), distances.dist)

let add = actions.add(metadata, preprocessed, index)
# let search = actions.search(preprocessed, index)
# TODO actions don't inherit their config name. Should they?
let echoto = actions.echoto(metadata, preprocessed)

export add
# export search
export echoto
